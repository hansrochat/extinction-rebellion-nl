{
  "name": "roots/bedrock",
  "type": "project",
  "license": "MIT",
  "description": "WordPress boilerplate with modern development tools, easier configuration, and an improved folder structure",
  "homepage": "https://roots.io/bedrock/",
  "authors": [
    {
      "name": "Scott Walkinshaw",
      "email": "scott.walkinshaw@gmail.com",
      "homepage": "https://github.com/swalkinshaw"
    },
    {
      "name": "Ben Word",
      "email": "ben@benword.com",
      "homepage": "https://github.com/retlehs"
    }
  ],
  "keywords": [
    "bedrock",
    "composer",
    "roots",
    "wordpress",
    "wp",
    "wp-config"
  ],
  "support": {
    "issues": "https://github.com/roots/bedrock/issues",
    "forum": "https://discourse.roots.io/category/bedrock"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist"
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    },
    {
      "type": "vcs",
      "url": "https://github.com/ggamel/wp-sync-db"
    },
    {
      "type": "package",
      "package": {
          "name": "advanced-custom-fields/advanced-custom-fields-pro",
        "version": "5.11",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://connect.advancedcustomfields.com/index.php?a=download&p=pro&k={%PLUGIN_ACF_KEY}&t={%VERSION}"
        },
        "require": {
          "composer/installers": "^1.4",
          "ffraenz/private-composer-installer": "^5.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "wpml/wpml-multilingual-cms",
        "version": "4.5.1",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=6088&user_id={%PLUGIN_WPML_ID}&subscription_key={%PLUGIN_WPML_KEY}&t=1636720760&version={%VERSION}"
        },
        "require": {
          "composer/installers": "^1.4",
          "ffraenz/private-composer-installer": "^5.0"
        }
      }
    },

    {
      "type": "package",
      "package": {
        "name": "wpml/wpml-string-translation",
        "version": "3.2.0",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=6092&user_id={%PLUGIN_WPML_ID}&subscription_key={%PLUGIN_WPML_KEY}&t=1636720760&version={%VERSION}"
        },
        "require": {
          "composer/installers": "^1.4",
          "ffraenz/private-composer-installer": "^5.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "wpml/wpml-acf-multilingual",
        "version": "1.9.4",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=1097589&user_id={%PLUGIN_WPML_ID}&subscription_key={%PLUGIN_WPML_KEY}&t=1636720760&version={%VERSION}"
        },
        "require": {
          "composer/installers": "^1.4",
          "ffraenz/private-composer-installer": "^5.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "wpml/wpml-yoast-seo-multilingual",
        "version": "2.0.1",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=3566177&user_id={%PLUGIN_WPML_ID}&subscription_key={%PLUGIN_WPML_KEY}&t=1637088836&version={%VERSION}"
        },
        "require": {
          "composer/installers": "^1.4",
          "ffraenz/private-composer-installer": "^5.0"
        }
      }
    },
    {
      "type": "package",
      "package": {
        "name": "wpml/wpml-woo-commerce-multilingual",
        "version": "4.12.1",
        "type": "wordpress-plugin",
        "dist": {
          "type": "zip",
          "url": "https://wpml.org/?download=637370&user_id={%PLUGIN_WPML_ID}&subscription_key={%PLUGIN_WPML_KEY}&t=1637088836&version={%VERSION}"
        },
        "require": {
          "composer/installers": "^1.4",
          "ffraenz/private-composer-installer": "^5.0"
        }
      }
    }
  ],
  "require": {
    "php": ">=7.1",
    "composer/installers": "1.10 as 1.0.6",
    "vlucas/phpdotenv": "^5.2",
    "oscarotero/env": "^2.1",
    "roots/bedrock-autoloader": "^1.0",
    "roots/wordpress": "5.8.*",
    "roots/wp-config": "1.0.0",
    "roots/wp-password-bcrypt": "1.0.0",
    "wpackagist-plugin/open-external-links-in-a-new-window": "^1.4.2",
    "wpackagist-plugin/display-posts-shortcode": "^3.0.2",
    "wpackagist-plugin/classic-editor": "^1.6",
    "wpackagist-plugin/events-manager": "^5.9.10",
    "wpackagist-plugin/wp-action-network": "^1.2.1",
    "wpackagist-plugin/wordpress-seo": "^17.0",
    "wpackagist-plugin/duplicate-post": "^4.1.1",
    "wpackagist-plugin/import-meetup-events": "^1.4.3",
    "wpackagist-plugin/wp-super-cache": "^1.7",
    "wpackagist-plugin/redirection": "^5.0.1",
    "wpackagist-plugin/simple-history": "^2.39.0",
    "wpackagist-plugin/robin-image-optimizer": "^1.5.0",
    "wpackagist-plugin/safe-svg": "^1.9.9",
    "wpackagist-plugin/woocommerce": "^5.9",
    "advanced-custom-fields/advanced-custom-fields-pro": "*",
    "wpml/wpml-multilingual-cms": "*",
    "wpml/wpml-string-translation": "*",
    "wpml/wpml-acf-multilingual": "*",
    "wpml/wpml-yoast-seo-multilingual": "*",
    "wpml/wpml-woo-commerce-multilingual": "*",
    "ggamel/wp-sync-db": "dev-master",
    "wp-sync-db/wp-sync-db-media-files": "^1.1",
    "wpackagist-plugin/mollie-payments-for-woocommerce": "^6.7"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.5.8",
    "roave/security-advisories": "dev-master"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "installer-paths": {
      "web/app/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "web/app/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "web/app/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    },
    "wordpress-install-dir": "web/wp"
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"copy('.env.example', '.env');\""
    ],
    "test": [
      "phpcs"
    ],
    "post-install-cmd": [
      "cd web/app/themes/xrnl; npm install",
      "cd web/app/themes/xrnl; npm run production"
    ]
  }
}
